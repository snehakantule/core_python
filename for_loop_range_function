#---------------for loop--------
x="sneha"
for letters in x :
    print(letters)
friends = ["sneha", "rohoni", "shivani", "vishal", "sweety"]
for element in friends :
    print(element)

#-----------Range function in for loop----------
for numbers in range(1,10):
    print(numbers)
for numbers in range(5):
    print(numbers)
for numbers in range (1,51):
    print(numbers)
