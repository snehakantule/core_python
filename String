#Data struture
#-------string--------
#--------Functions of string-------
x="sneha"
#--------upper-------
print(x.upper())
#--------lower--------
print(x.lower())
#---------isupper---
print(x.isupper())
#-------islower-----
print(x.islower())
#-------len-----
y=len("sweety")
print(y)
#-------replace----
print(x.replace("sneha","rohini"))
#--------index-----
print(x.index("e"))


